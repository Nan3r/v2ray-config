{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "protocol": "vmess",
      "port": 22777,
      "settings": {
        "clients": [
          {
            "id": "uuid",//uuid保持与客户端的一致
            "alterId": 128
          }
        ]
      }
    },
    {
      "protocol": "vmess",
      "port": 22778,
      "tag": "tor",
      "settings": {
        "clients": [
          {
            "id": "uuid",//uuid保持与客户端的一致
            "alterId": 128
          }
        ]
      }
    },
    {
      "protocol": "shadowsocks",
      "port": 9888,
      "settings": {
        "method": "aes-128-cfb",
        "password": "xxxx",
        "network": "tcp,udp"
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "freedom",
      "settings": {}
    },
    {
      "protocol": "socks",
      "tag": "torout",
      "settings": {
        "servers": [
          {
            "address": "127.0.0.1",
            "port": 9050
          }
        ]
      }
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "blocked"
    }
  ],
  "routing": {
    "strategy": "rules",
    "settings": {
      "rules": [
        {
          "type": "field",
          "ip": [
            "0.0.0.0/8",
            "10.0.0.0/8",
            "100.64.0.0/10",
            "127.0.0.0/8",
            "169.254.0.0/16",
            "172.16.0.0/12",
            "192.0.0.0/24",
            "192.0.2.0/24",
            "192.168.0.0/16",
            "198.18.0.0/15",
            "198.51.100.0/24",
            "203.0.113.0/24",
            "::1/128",
            "fc00::/7",
            "fe80::/10"
          ],
          "outboundTag": "blocked"
        },
        {
          "inboundTag": [
            "tor"
          ],
          "type": "field",
          "outboundTag": "torout"
        }
      ]
    }
  }
}
